<?xml version="1.0" encoding="utf-8" ?> 
<configuration>
	<configSections>
		<sectionGroup name="CCNet">
			<section name="buildPlugins" type="ThoughtWorks.CruiseControl.WebDashboard.Config.PluginsSectionHandler,ThoughtWorks.CruiseControl.WebDashboard" />
			<section name="projectPlugins" type="ThoughtWorks.CruiseControl.WebDashboard.Config.PluginsSectionHandler,ThoughtWorks.CruiseControl.WebDashboard" />
			<section name="serverPlugins" type="ThoughtWorks.CruiseControl.WebDashboard.Config.PluginsSectionHandler,ThoughtWorks.CruiseControl.WebDashboard" />
			<section name="farmPlugins" type="ThoughtWorks.CruiseControl.WebDashboard.Config.PluginsSectionHandler,ThoughtWorks.CruiseControl.WebDashboard" />
		</sectionGroup>
		<section name="xslFiles" type="ThoughtWorks.CruiseControl.Core.Config.XslFilesSectionHandler,ThoughtWorks.CruiseControl.Core"/>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="cruiseServer" type="ThoughtWorks.CruiseControl.Core.Config.ServerConfigurationHandler,ThoughtWorks.CruiseControl.Core"/>
  </configSections>
	<xslFiles>
		<file name="xsl\header.xsl"/>
		<file name="xsl\compile.xsl"/>
		<file name="xsl\unittests.xsl"/>
		<file name="xsl\fit.xsl"/>
		<file name="xsl\modifications.xsl"/>
	</xslFiles>
  <cruiseServer>
    <extension type="ThoughtWorks.CruiseControl.UnitTests.Remote.ServerExtensionStub,ThoughtWorks.CruiseControl.UnitTests">
      <testData/>
    </extension>
  </cruiseServer>
  <CCNet>
		<buildPlugins>
			<plugin typeName="ThoughtWorks.CruiseControl.UnitTests.WebDashboard.Dashboard.StubBuildPlugin" assemblyFileName="ThoughtWorks.CruiseControl.UnitTests.dll" />
		</buildPlugins>
		<projectPlugins>
			<plugin typeName="ThoughtWorks.CruiseControl.UnitTests.WebDashboard.Dashboard.StubBuildPlugin" assemblyFileName="ThoughtWorks.CruiseControl.UnitTests.dll" />
		</projectPlugins>
		<serverPlugins>
			<plugin typeName="ThoughtWorks.CruiseControl.UnitTests.WebDashboard.Dashboard.StubBuildPlugin" assemblyFileName="ThoughtWorks.CruiseControl.UnitTests.dll" />
		</serverPlugins>
		<farmPlugins>
			<plugin typeName="ThoughtWorks.CruiseControl.UnitTests.WebDashboard.Dashboard.StubBuildPlugin" assemblyFileName="ThoughtWorks.CruiseControl.UnitTests.dll" />
		</farmPlugins>
	</CCNet>

	<log4net>
		<root>
			<!-- 
				Change the value in the next line to alter the amount of logging generated by CruiseControl.NET.
				The following values are supported: DEBUG, INFO, WARN, ERROR, OFF.
				For more detailed information on how to configure log4net, see http://logging.apache.org/log4net/
				 -->
			<level value="OFF" />
			<appender-ref ref="Console" />
		</root>
		<appender name="Console" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%thread:%level] %message%newline" />
			</layout>
		</appender>
	</log4net>
</configuration>
