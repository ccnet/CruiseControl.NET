<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
	<head>
		<title>CruiseControl.NET</title>
		<link type="text/css" rel="stylesheet" href="$applicationPath/cruisecontrol.css" />
		<link type="text/css" rel="stylesheet" href="$applicationPath/javascript/theme/ui.all.css" />
		<link rel="shortcut icon" href="$applicationPath/AppIcon.ico" />
		<link type="text/css" rel="stylesheet" href="$applicationPath/javascript/highlight/styles/github.css">
		<meta http-equiv="refresh" content="$refreshinterval" > 		
		<script src="$applicationPath/javascript/highlight/highlight.pack.js"></script>		
		<script src="$applicationPath/javascript/ccnet.js" type="text/javascript"></script>
		<script src="$applicationPath/javascript/jquery.js" type="text/javascript"></script>
		<script src="$applicationPath/javascript/jquery-ui.js" type="text/javascript"></script>
		<script src="$applicationPath/javascript/jquery.metadata.js" type="text/javascript"></script>
		<script src="$applicationPath/javascript/jquery.tablesorter.js" type="text/javascript"></script>
		<script src="$applicationPath/javascript/jQuery.ccnet.js" type="text/javascript"></script>
	</head>
	<body class="wholepage">
		<table class="main-panel" border="0" width="100%">
			<tr>
				<td valign="middle" align="left">
					<a href="http://ccnet.thoughtworks.com"><img src="$applicationPath/images/ccnet_logo.gif" border="0" alt="$translations.Translate("CruiseControlNETLogo")" /></a>
				</td>
				<td align="right" valign="bottom">
					$loginView
					<a href="$applicationPath/doc/index.html">$translations.Translate("Documentation")</a>
				</td>
			</tr>
		</table>
		<table class="TopControls" border="0" width="100%" cellpadding="3" cellspacing="0">
			<tr>
				<td valign="middle" align="left">$breadcrumbs</td>
				<td valign="middle" align="right"><p>$translations.Translate("Version") : $dashboardversion</p></td>
			</tr>
		</table>
		<table border="0" align="center" cellpadding="0" cellspacing="0" width="100%">
			<tr>
				<td valign="top" id="LeftHandSide" bgcolor="#eeeedd" width="180">
					<table id="Plugins" cellpadding="3" cellspacing="0" border="0">
						<tr><td><img src="$applicationPath/images/shim.gif" width="180" height="1" alt="" /></td></tr>
						<tr><td>$sidebar</td></tr>
					</table>
				</td>
				<td width="2"><img src="$applicationPath/images/shim.gif" alt="" width="2"/></td>
				<td valign="top" bgcolor="#ffffff">
					<table border="0" align="center" cellpadding="3" cellspacing="0" width="100%">
						<tr><td>
						<!-- Main content: Start -->
						$mainContent
						<!-- Main content: End -->
						</td></tr>
					</table>
				</td>
			</tr>
			<tr>
				<td height="2" colspan="3"></td>
			</tr>
		</table>
		<table class="Footer" width="100%">
			<tr valign="top">
				<td>
					<p>$translations.Translate("PageRendered", $renderedAt)</p>
				</td>
				<td align="right">
					<a href="http://opensource.thoughtworks.com/"><img src="$applicationPath/images/tw_open_source_small.png" border="0" alt="ThoughtWorks Open Source"/></a>
				</td>
			</tr>
		</table>
	</body>
</html>
