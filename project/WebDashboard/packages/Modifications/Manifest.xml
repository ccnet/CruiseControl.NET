<package>
  <name>Modifications</name>
  <description>Display the modifications done in the build.</description>
  <type>Plugin</type>
  <group>Build</group>  
  <folders>
    <folder>
      <location>Xsl</location>
      <files>
        <file>modifications.xsl</file>
      </files>
    </folder>
  </folders>
  <configuration>
    <setting>
      <path>/dashboard/plugins/buildPlugins/buildReportBuildPlugin/xslFileNames</path>
      <filter>.='xsl\modifications.xsl'</filter>
      <name>xslFile</name>
      <value>xsl\modifications.xsl</value>
    </setting>  
  </configuration>
</package>