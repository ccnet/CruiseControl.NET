<package>
  <name>Console Output</name>
  <description>Displays the output of a build like the console (no-xml)</description>
  <type>Plugin</type>
  <group>Build</group>    
  <folders>
    <folder>
      <location>Xsl</location>
      <files>
        <file>console.xsl</file>
      </files>
    </folder>
  </folders>
  <configuration>
    <setting>
      <path>/dashboard/plugins/buildPlugins</path>
      <filter>@xslFileName='xsl\Console.xsl'</filter>
      <name>xslReportBuildPlugin</name>
      <attributes>
        <attribute name="description" value="Console Output"/>
        <attribute name="actionName" value="ConsoleOutputBuildReport"/>
        <attribute name="xslFileName" value="xsl\Console.xsl"/>
      </attributes>
    </setting>
  </configuration>
</package>